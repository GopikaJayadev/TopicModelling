   "source": [
    "import pandas\n",
    "Questions = pandas.DataFrame([1,2,3,4,5,6])\n",
    "Questions.columns = ['QuestionNumber']\n",
    "Questions['value'] = pandas.Series([6,5,8,4,9,2])\n",
    "Questions['prob'] = pandas.Series([0.4,0.5,0.2,0.6,0.3,0.9])\n",
    "Questions['greedy'] = Questions.value*Questions.prob\n",
    "Questions['indexPol'] = Questions.value*(Questions.prob/(1-Questions.prob))\n",
    "Greedy = Questions.sort_values(by = ['greedy'], ascending= False)\n",
    "indexPol = Questions.sort_values(by = ['indexPol'], ascending= False)\n",
    "\n",
    "\n",
    "def ExpVal(df,L):\n",
    "    s =0 \n",
    "    x = 1\n",
    "    for i in L:\n",
    "        prob = df.iloc[i-1]['prob']\n",
    "        value = df.iloc[i-1]['value']\n",
    "        x = x*prob\n",
    "        s = s + x*value    \n",
    "    return s\n",
    "\n",
    "import copy\n",
    "def rollout(df,order,M):\n",
    "    sel_order = []\n",
    "    rem_order = copy.deepcopy(order)\n",
    "    for j in range(M):\n",
    "        print 'Iteration : ' + str(j)\n",
    "        maxval = 0\n",
    "        maxindex = 0\n",
    "        for i in range(len(rem_order)):\n",
    "            L = sel_order + [rem_order[i]] + rem_order[0:i] + rem_order[i+1::]\n",
    "            val = ExpVal(df,L)\n",
    "            print 'Order : ' + str(L)\n",
    "            print 'Value : ' + str(val)\n",
    "            if maxval < val:\n",
    "                maxval = val\n",
    "                maxindex = i\n",
    "        sel_order = sel_order + [rem_order[maxindex]]\n",
    "        print 'Selected order : ' + str(sel_order)\n",
    "        rem_order =  rem_order[0:maxindex] + rem_order[maxindex+1::]\n",
    "        \n",
    "    return sel_order\n",
    "   \n",
    "\n",
    "print 'Index Policy output :'\n",
    "rollout(Questions,list(indexPol.QuestionNumber),3)\n",
    "print 'Greedy Policy Output :'\n",
    "rollout(Questions,list(Greedy.QuestionNumber),3)"
   ]
  },
